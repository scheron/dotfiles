{
    "vsicons.presets.hideExplorerArrows": true,
    "catppuccin-icons.hidesExplorerArrows": true,
    "custom-ui-style.font.sansSerif": "Iosevka",
    "editor.fontFamily": "Geist Mono, Zenbones Mono, Fragment Mono, Test Söhne Mono, SF Mono, Lekton Nerd Font Mono, ZedMono Nerd Font, RecMonoLinear Nerd Font, Maple Mono, Monoid Nerd Font, SauceCodePro Nerd Font Mono, FiraCode Nerd Font, ProFontWindows Nerd Font Mono, Iosevka, IosevkaTermSlab Nerd Font, Victor Mono, MesloLGS Nerd Font, JetBrainsMono Nerd Font Mono",
    "terminal.integrated.fontFamily": "ZedMono Nerd Font",
    "terminal.integrated.fontSize": 13,
    "editor.fontSize": 15,
    "editor.lineHeight": 1.5,
    "editor.fontWeight": 500,
    "diffEditor.ignoreTrimWhitespace": true,
    "diffEditor.renderSideBySide": true,
    "git-graph.commitDetailsView.location": "Docked to Bottom",
    "git-graph.dialog.addTag.type": "Lightweight",
    "git-graph.graph.style": "angular",
    "git-graph.graph.uncommittedChanges": "Open Circle at the Checked Out Commit",
    "git-graph.referenceLabels.alignment": "Branches (aligned to the graph) & Tags (on the right)",
    "git-graph.repository.commits.fetchAvatars": true,
    "git.openRepositoryInParentFolders": "never",
    "javascript.preferences.renameMatchingJsxTags": false,
    "typescript.preferences.renameMatchingJsxTags": false,
    "editor.formatOnSave": false,
    "prettier.requireConfig": true,
    "prettier.useEditorConfig": false,
    "files.autoSaveWhenNoErrors": true,
    "editor.accessibilitySupport": "off",
    "extensions.ignoreRecommendations": true,
    "cSpell.userWords": [
        "abortable",
        "aggr",
        "clsx",
        "composables",
        "endregion",
        "filedata",
        "Finres",
        "fullscreen",
        "maxt",
        "nsec",
        "Nuxt",
        "pinia",
        "primevue",
        "unnessasery",
        "unsub",
        "Vaul",
        "vueuse",
        "Вопросы"
    ],
    "terminal.explorerKind": "both",
    "terminal.integrated.env.osx": {
        "FIG_NEW_SESSION": "1"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[vue]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "supermaven.enableFixWithSupermaven": true,
    "workbench.iconTheme": "charmed-warm",
    "workbench.colorTheme": "Tokyo Night",
    "vim.leader": "<space>",
    "vim.useSystemClipboard": true,
    "editor.lineNumbers": "relative",
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.hlsearch": true,
    "vim.foldfix": true,
    "vim.insertModeKeyBindings": [
        { "before": [ "j", "j" ], "after": [ "<esc>" ] },
        { "before": [ "j", "k" ], "after": [ "<esc>" ] },
        { "before": [ "<C-i>" ], "commands": [ "editor.action.triggerSuggest" ] }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        { "before": [ "leader", "f", "s" ], "commands": [ "workbench.action.gotoSymbol" ] },
        { "before": [ "leader", "f", "f" ], "commands": [ "workbench.action.quickOpen" ] },
        { "before": [ "leader", "f", "w" ], "commands": [ "workbench.action.findInFiles" ] },
        { "before": [ "leader", "f", "b" ], "commands": [ "workbench.action.showAllEditors" ] },
        { "before": [ "leader", "f", "r" ], "commands": [ "workbench.action.showAllEditorsByMostRecentlyUsed" ] },
        { "before": [ "<S-h>" ], "commands": [ ":bprevious" ] },
        { "before": [ "<S-l>" ], "commands": [ ":bnext" ] },
        { "before": [ "<S-Tab>" ], "commands": [ ":bprevious" ] },
        { "before": [ "<Tab>" ], "commands": [ ":bnext" ] },
        { "before": [ "leader", "|" ], "commands": [ ":vsplit" ] },
        { "before": [ "leader", "-" ], "commands": [ ":split" ] },
        { "before": [ "leader", "h" ], "commands": [ "workbench.action.focusLeftGroup" ] },
        { "before": [ "leader", "j" ], "commands": [ "workbench.action.focusBelowGroup" ] },
        { "before": [ "leader", "k" ], "commands": [ "workbench.action.focusAboveGroup" ] },
        { "before": [ "leader", "l" ], "commands": [ "workbench.action.focusRightGroup" ] },
        { "before": [ "leader", "w" ], "commands": [ ":w!" ] },
        { "before": [ "leader", "q" ], "commands": [ ":q!" ] },
        { "before": [ "Q" ], "commands": [ "workbench.action.closeAllEditors" ] },
        { "before": [ "leader", "x" ], "commands": [ ":x!" ] },
        { "before": [ "[", "d" ], "commands": [ "editor.action.marker.prev" ] },
        { "before": [ "]", "d" ], "commands": [ "editor.action.marker.next" ] },
        { "before": [ "leader", "s", "i" ], "commands": [ "editor.action.showDefinitionPreviewHover" ] },
        { "before": [ "leader", "s", "a" ], "commands": [ "editor.action.codeAction" ] },
        { "before": [ "leader", "s", "d" ], "commands": [ "editor.action.revealDefinition" ] },
        { "before": [ "leader", "p" ], "commands": [ "editor.action.formatDocument" ] },
        { "before": [ "leader", "o" ], "commands": [ "editor.action.organizeImports" ] },
        { "before": [ "leader", "r" ], "commands": [ "editor.action.rename" ] },
        { "before": [ "leader", "/" ], "commands": [ "editor.action.commentLine" ] },
        { "before": [ "<leader>", "e" ], "commands": [ "workbench.explorer.fileView.focus", "workbench.files.action.showActiveFileInExplorer" ] }
    ],
    "vim.visualModeKeyBindings": [
        { "before": [ "<" ], "commands": [ "editor.action.outdentLines" ] },
        { "before": [ ">" ], "commands": [ "editor.action.indentLines" ] },
        { "before": [ "J" ], "commands": [ "editor.action.moveLinesDownAction" ] },
        { "before": [ "K" ], "commands": [ "editor.action.moveLinesUpAction" ] }
    ],
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false
    },
    "extensions.experimental.affinity": {
        "vscodevim.vim": 1,
        "asvetliakov.vscode-neovim": 1
    },
    "editor.suggest.insertMode": "insert",
    "editor.glyphMargin": false,
    "editor.minimap.enabled": false,
    "explorer.compactFolders": false,
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.vertical": "hidden",
    "editor.scrollbar.horizontalScrollbarSize": 0,
    "editor.scrollbar.verticalScrollbarSize": 10,
    "symbols.hidesExplorerArrows": true,
    "material-icon-theme.hidesExplorerArrows": true,
    "workbench.tree.renderIndentGuides": "none",
    "workbench.tips.enabled": false,
    "editor.lightbulb.enabled": "off",
    "editor.showFoldingControls": "never",
    "editor.overviewRulerBorder": false,
    "editor.cursorBlinking": "solid",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.fontLigatures": false,
    "editor.renderWhitespace": "none",
    "editor.renderControlCharacters": false,
    "editor.stickyScroll.enabled": false,
    "editor.stickyScroll.scrollWithEditor": false,
    "workbench.tree.enableStickyScroll": false,
    "workbench.startupEditor": "none",
    "zenMode.hideLineNumbers": false,
    "breadcrumbs.symbolPath": "off",
    "editor.codeLens": false,
    "editor.linkedEditing": true,
    "workbench.editor.enablePreview": true,
    "editor.renderLineHighlight": "none",
    "javascript.updateImportsOnFileMove.enabled": "never",
    "workbench.settings.applyToAllProfiles": [],
    "gitlens.ai.experimental.model": "openai:gpt-4o",
    "update.mode": "manual",
    "animations.Enabled": true,
    "screencastMode.verticalOffset": 30,
    "editor.cursorSurroundingLines": 10,
    "database-client.autoSync": true,
    "cSpell.language": "en,ru",
    "vscodeCustomCodeColor.fileLanguageIdList": [
        "vue",
        "typescript"
    ],
    "vscodeCustomCodeColor.highlightValue": "v-role",
    "vscodeCustomCodeColor.highlightValueColor": "#6366f1",
    "update.showReleaseNotes": false,
    "window.titleBarStyle": "custom",
    "window.zoomLevel": 1.5,
    "editor.quickSuggestionsDelay": 100,
    "workbench.hover.delay": 1500,
    "workbench.sash.hoverDelay": 1000,
    "workbench.productIconTheme": "fluent-icons",
    "editor.tabSize": 2,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.pinnedTabsOnSeparateRow": true,
    "workbench.fontAliasing": "auto",
    "workbench.view.alwaysShowHeaderActions": true,
    "workbench.view.showQuietly": {
        "workbench.panel.output": false
    },
    "workbench.editor.alwaysShowEditorActions": true,
    "workbench.editor.closeEmptyGroups": false,
    "workbench.navigationControl.enabled": false,
    "workbench.layoutControl.enabled": false,
    "git.autofetch": true,
    "git.confirmSync": false,
    "vscode_vibrancy.forceFramelessWindow": true,
    "vscode_vibrancy.theme": "Catppuccin Mocha",
    "vscode_vibrancy.type": "auto",
    "vscode_vibrancy.disableThemeFixes": false,
    "vscode_vibrancy.opacity": "-1",
    "vscode_vibrancy.preventFlash": true,
    "vscode_vibrancy.enableAutoTheme": false,
    "workbench.colorCustomizations": {
        "terminal.background": "#00000000"
    },
    "terminal.integrated.gpuAcceleration": "off",
    "window.systemColorTheme": "dark",
    "window.autoDetectColorScheme": false
}
